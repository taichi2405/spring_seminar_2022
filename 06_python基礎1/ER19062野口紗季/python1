      "cell_type": "markdown",
      "source": [
        "#課題2\n",
        "3つの活性化関数をまとめて表示"
      ],
      "metadata": {
        "id": "vysNHDDf6V68"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def sigmoid(x):\n",
        "    return 1/(1+np.exp(-x))\n",
        "\n",
        "def softmax(x):\n",
        "    exp_x = np.exp(x)\n",
        "    sum_exp_x = np.sum(exp_x)\n",
        "    return exp_x/sum_exp_x\n",
        "\n",
        "def tanh(x):\n",
        "    exp_x1 = np.exp(x)\n",
        "    exp_x2 = np.exp(-x)\n",
        "    upper = exp_x1 - exp_x2\n",
        "    lower = exp_x1 + exp_x2\n",
        "    return upper/lower\n",
        "\n",
        "x = np.arange(-5.0, 5.0, 0.1)\n",
        "y1 = sigmoid(x)\n",
        "y2 = softmax(x)\n",
        "y3= tanh(x)\n",
        "\n",
        "plt.plot(x,y1)\n",
        "plt.plot(x,y2)\n",
        "plt.plot(x,y3)\n",
        "\n",
        "plt.ylim(-1.2, 1.2)\n",
        "\n",
        "plt.plot(x, y1, label='sigmoid')\n",
        "plt.plot(x, y2, label='softmax')\n",
        "plt.plot(x, y3, label='tanh')\n",
        "\n",
        "plt.xlabel('input')\n",
        "plt.ylabel('output')\n",
        "plt.legend()\n",
        "plt.show()"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 279
