        "#課題1\n",
        "tanh関数をグラフにする"
      ],
      "metadata": {
        "id": "R95QlJ_b6LJQ"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def tanh(x):\n",
        "    exp_x1 = np.exp(x)\n",
        "    exp_x2 = np.exp(-x)\n",
        "    upper = exp_x1 - exp_x2\n",
        "    lower = exp_x1 + exp_x2\n",
        "    return upper/lower\n",
        "\n",
        "x = np.arange(-5.0, 5.0, 0.1)\n",
        "y= tanh(x)\n",
        "\n",
        "plt.plot(x,y)\n",
        "plt.ylim(-1.2, 1.2)\n",
        "\n",
        "plt.title('tanh')\n",
        "plt.xlabel('input')\n",
        "plt.ylabel('output')\n",
        "plt.show()"
      ],
      "metadata": {
        "id": "eaDZANQF1IBh",
