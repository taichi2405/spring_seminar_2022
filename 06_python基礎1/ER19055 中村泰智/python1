        "\n",
        "img=cv2.imread('black2.jpeg')\n",
        "points = np.array([(60, 100), (110, 30), (160, 100)])\n",
        "cv2.fillConvexPoly(img, points, (0, 255, 255))\n",
        "points = np.array([(60, 100), (10, 170), (110, 170)])\n",
        "cv2.fillConvexPoly(img, points, (0, 255, 255))\n",
        "points = np.array([(160, 100), (210, 170), (110, 170)])\n",
